	"version": "0.2.0",
	"configurations": [
		{
			"name": "castle-on-the-grid",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": [
				"src/medium/castle-on-the-grid/test/**/*.js",
